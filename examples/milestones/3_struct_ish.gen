;; { x : i32, y : i32, w : i32, h : i32 }
(def rect_init (fun (p x y w h) (
  (store_32 p x)
  (store_32 (+ p 4) y)
  (store_32 (+ p 8) w)
  (store_32 (+ p 12) h)
)))

(def main (fun () (
  (let rect (alloca 16))
  (rect_init rect 1 2 3 4)

  (if (ccall test_struct rect)
    ((ccall print_symbol (sym SUCCESS)))
    ((ccall print_symbol (sym FAILED)))
  )
)))
